{
  "version": 2,
  "buildCommand": "cd anonymous-chat && npm run build",
  "devCommand": "cd anonymous-chat && npm run dev",
  "installCommand": "cd anonymous-chat && npm install",
  "outputDirectory": "anonymous-chat/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
} 